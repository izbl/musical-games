<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/flappy_navigation"
    app:startDestination="@id/fragmentLevelList">

    <fragment
        android:id="@+id/flappyGameFragment"
        android:name="com.example.musicalgames.games.flappy.GameFragment"
        android:label="FlappyGameFragment" >

        <action
            android:id="@+id/action_flappyGameFragment_to_gameEndedFragment"
            app:destination="@id/gameEndedFragment">
        </action>
    </fragment>
    <fragment
        android:id="@+id/gameEndedFragment"
        android:name="com.example.musicalgames.games.flappy.GameEndedFragment"
        android:label="GameEndedFragment" >
        <action
            android:id="@+id/action_gameEndedFragment_to_flappyGameFragment"
            app:destination="@id/flappyGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLevelList"
        android:name="com.example.musicalgames.games.flappy.level_list.FragmentLevelList"
        android:label="FragmentLevelList" >
        <action
            android:id="@+id/action_fragmentLevelList_to_flappyGameFragment"
            app:destination="@id/flappyGameFragment" />
    </fragment>
</navigation>